<Window x:Class="BarcodeReader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="278" Width="525">
    <Grid Height="231">
        <TextBox Height="126" Name="textBox1" TextWrapping="Wrap" VerticalAlignment="Top" Margin="0,36,0,0" />
        <Button Content="Scan Barcode" Height="35" HorizontalAlignment="Left" Margin="192,0,0,12" Name="scan" VerticalAlignment="Bottom" Width="121" Click="button2_Click" />
        <Label  Content="Push Button to scan barcode, wait for video, when text below contains text, open ETS and past text into 'Scan Barcode' field" Height="73" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" Width="414" />
        <Button Content="Button" Height="23" HorizontalAlignment="Left" Margin="384,204,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" />
    </Grid>  
</Window>
